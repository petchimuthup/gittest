<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>

        <link src="Tychons_Favoriteorder::js/movestoreorder.js"/>
        <link src="Tychons_Favoriteorder::js/favoriteorder.js"/>
        <link src="Tychons_Favoriteorder::js/productdelete.js"/>

    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Favorite Products</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Tychons\Favoriteorder\Block\FavoriteOrderProduct" name="favoriteorder.product.list"
                   template="Tychons_Favoriteorder::favoriteproduct.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>