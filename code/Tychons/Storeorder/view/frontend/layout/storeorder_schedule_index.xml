<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
<head>
  <link src="Tychons_Storeorder::js/storeschedule.js"/>
</head>
    <update handle="customer_account"/>
    <body>
		<referenceBlock name="page.main.title">
		    <action method="setPageTitle">
		        <argument translate="true" name="title" xsi:type="string">Order Schedule</argument>
		    </action>
		</referenceBlock>
        <referenceContainer name="content">
            <block class="Tychons\Storeorder\Block\Schedule\Index" name="schedule.customer.list" template="Tychons_Storeorder::schedule/index.phtml" cacheable="false" />
        </referenceContainer>
    </body>
</page>