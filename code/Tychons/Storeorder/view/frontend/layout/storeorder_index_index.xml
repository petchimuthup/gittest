<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
<head>
  <link src="Tychons_Storeorder::js/storeorder.js"/>

</head>
    <body>
		<referenceBlock name="page.main.title">
		    <action method="setPageTitle">
		        <argument translate="true" name="title" xsi:type="string">Store Order</argument>
		    </action>
		</referenceBlock>
        <referenceContainer name="content">
            <block class="Tychons\Storeorder\Block\StoreOrderProduct" name="storeorder.product.list" template="Tychons_Storeorder::storeproduct.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>